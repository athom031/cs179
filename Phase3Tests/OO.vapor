const vmt_OO
  :OO.Init
  :OO.Print

func Main()
  x = HeapAllocZ(20)
  [x+0] = :vmt_OO
  v = [x]
  v = [v+0]
  v = call v(x 2 4 6)
  v = [x]
  v = [v+4]
  v = call v(x)
  ret


func OO.Init(this x y z)
  [this+4] = x
  [this+8] = y
  [this+12] = z
  ret 0

func OO.Print(this)
  x = [this+4]
  PrintIntS(x)
  y = [this+8]
  PrintIntS(y)
  z = [this+12]
  PrintIntS(z)
  ret 0
